@{
    ViewBag.Title = "สิ่งปลูกสร้าง";
    ViewBag.Header = "";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="panel-heading"><label>สิ่งปลูกสร้าง</label></div>
<div class="panel-body">
@using (Html.BeginForm())
