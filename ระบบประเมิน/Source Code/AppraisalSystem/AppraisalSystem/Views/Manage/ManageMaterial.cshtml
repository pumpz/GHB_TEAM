                    <label>ขนาดสิ่งปลูกสร้าง</label>
                </label>
            </div>
            <div class="form-group">
                <label class="textbox-inline">
                     <select class="form-control">
                        <option>โปรดเลือก</option>
                        <option>2</option>
                        <option>3</option>
                        <option>4</option>
                        <option>5</option>
                    </select>
